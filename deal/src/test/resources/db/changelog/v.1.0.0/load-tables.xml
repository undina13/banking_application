<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">

    <changeSet id="load_tables" author="undina">
        <loadData encoding="UTF-8" file="db/init_data/client.csv" tableName="client" separator=";">
            <column name="client_id" type="NUMERIC"/>
            <column name="last_name" type="STRING"/>
            <column name="first_name" type="STRING"/>
            <column name="middle_name" type="STRING"/>
            <column name="birth_date" type="DATE 'YYYY-MM-DD'"/>
            <column name="email" type="STRING"/>
            <column name="passport" type="STRING"/>
        </loadData>

      <loadData encoding="UTF-8" file="db/init_data/application.csv" tableName="application" separator=";">
            <column name="application_id" type="NUMERIC"/>
          <column name="client_id" type="NUMERIC"/>
          <column name="status" type="STRING"/>
           <column name="creation_date" type="DATE 'YYYY-MM-DD'"/>
          <column name="status_history" type="STRING"/>

        </loadData>


    </changeSet>

</databaseChangeLog>